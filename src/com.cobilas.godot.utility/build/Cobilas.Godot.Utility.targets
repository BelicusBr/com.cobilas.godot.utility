<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<!--<Import Project="Cobilas.Godot.Utility.props"/>-->
	<UsingTask
		TaskName="Cobilas.GodotEditor.Utility.Tasks.GodotPluginTask"
		AssemblyFile="$(CurrentPathPackageFolder)\build\runtime\$(TargetFramework)\com.cobilas.godot.utility.tasks.dll"/>

	<!--AfterTargets="RunCobilasUtilityTask"-->
	<Target Name="RunCobilasPluginTask" BeforeTargets="BeforeCompile">
		
		<GodotPluginTask ProjectPath="$(MSBuildProjectDirectory)"/>
	</Target>

</Project>
